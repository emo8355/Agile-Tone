            "optional": false,
            "field": "/",
            "description": "<p>render the login page</p>"
          },
          {
            "group": "Success 200",
            "type": "post",
            "optional": false,
            "field": "/login",
            "description": "<p>render a login form</p>"
          },
          {
            "group": "Success 200",
            "type": "post",
